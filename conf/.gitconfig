[core]
  # 不转换换行符
  autocrlf = false
  safecrlf = true

  # 忽略换行符变更
  whitespace = cr-at-eol

# 加速
[http]
  proxy = socks5://127.0.0.1:10808
[https]
  proxy = socks5://127.0.0.1:10808

# 递归操作子项目
[submodule]
  recurse = true

[init]
  defaultBranch = main

[pull]
  rebase = false

[commit]
  gpgsign = true
[tag]
  gpgSign = true
[push]
  gpgSign = true
